<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/resul1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="39dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="31dp"
        android:text="" />

    <TextView
        android:id="@+id/resul2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/resul1"
        android:layout_marginLeft="92dp"
        android:layout_marginStart="92dp"
        android:layout_toEndOf="@+id/resul1"
        android:layout_toRightOf="@+id/resul1"
        android:text="" />

    <TextView
        android:id="@+id/resul3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resul2"
        android:layout_marginLeft="83dp"
        android:layout_marginStart="83dp"
        android:layout_toEndOf="@+id/resul2"
        android:layout_toRightOf="@+id/resul2"
        android:text="" />

    <ImageView
        android:id="@+id/copa1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/resul2"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        />

    <ImageView
        android:id="@+id/copa2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/copa1"
        android:layout_alignStart="@+id/copa1"
        android:layout_below="@+id/resul3"
        android:layout_marginTop="4dp"
         />

    <ImageView
        android:id="@+id/copa3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/copa2"
        android:layout_alignRight="@+id/copa2"
        android:layout_below="@+id/copa2"
        android:layout_marginTop="4dp"
         />


</RelativeLayout>